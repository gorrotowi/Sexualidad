<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.gorro.sexualidad" >

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:theme="@style/AppTheme" >
        <activity
            android:name=".SplashActivity"
            android:label="@string/app_name"
            android:theme="@style/AppThemeFull" >
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainSectionsActivity"
            android:label="@string/title_activity_main_sections" >
        </activity>
        <activity
            android:name=".HistorysActivity"
            android:label="@string/title_activity_historys" >
            <meta-data
                android:name="android.support.PARENT_ACTIVITY"
                android:value=".MainSectionsActivity" />
        </activity>
        <activity
            android:name=".NewsActivity"
            android:label="@string/title_activity_news" >
        </activity>
        <activity
            android:name=".MoreInfoActivity"
            android:label="@string/title_activity_more_info" >
        </activity>
        <activity
            android:name=".MoreInfoDescriptActivity"
            android:label="@string/title_activity_more_info_descript" >
        </activity>
    </application>

</manifest>
